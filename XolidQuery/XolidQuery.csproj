<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp2.2</TargetFramework>
        <RootNamespace>Dapper.XolidMapper</RootNamespace>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Authors>xolid</Authors>
        <Description>Dapper XML Query Mapper Plugin! Likes iBatis(MyBatis)</Description>
        <RepositoryUrl>https://github.com/xolidon/XolidQuery</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>Dapper XML Query MyBatis iBatis</PackageTags>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
        <PackageLicenseUrl>https://github.com/xolidon/XolidQuery/blob/master/XolidQuery/LICENSE</PackageLicenseUrl>
        <PackageVersion>1.0.3</PackageVersion>
        <SignAssembly>true</SignAssembly>
        <PublicSign>false</PublicSign>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Dapper" Version="1.60.6" />
      <PackageReference Include="NSoupCore" Version="1.0.0" />
    </ItemGroup>

